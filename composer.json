{
    "name": "rayxsi/framework",
    "description": "The rayxsi framework",
    "keywords": ["rayxsi", "framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "toerso",
            "email": "toerso.mechanix@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "src/Artificers/Support/helpers.php"
        ],
        "psr-4": {
            "Artificers\\": "src/Artificers"
        }
    },
    "require": {
        "php": "^8.0",
        "symfony/process": "6.1.x-dev",
        "vlucas/phpdotenv": "^5.4",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0",
        "symfony/http-foundation": "^6.1",
        "symfony/finder": "^6.1",
        "monolog/monolog": "^3.2",
        "voku/portable-ascii": "^2.0",
        "symfony/error-handler": "^7.0"
    },
    "replace": {
        "artificers/routing": "self.version",
        "artificers/view": "self.version",
        "artificers/treaties": "self.version",
        "artificers/filesystem": "self.version",
        "artificers/test": "self.version",
        "artificers/supports": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
